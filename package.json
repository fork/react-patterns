{
  "name": "react-patterns",
  "description": "Basic React Components, Hooks and Patterns, which we use at Fork",
  "keywords": [
    "react",
    "patterns",
    "storybook"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Fork Unstable Media GmbH",
    "email": "obj@fork.de"
  },
  "bugs": "https://github.com/fork/react-patterns/issues",
  "repository": "fork/react-patterns",
  "scripts": {
    "build": "build-storybook",
    "start": "start-storybook -s ./public -p 8000",
    "deploy": "yarn build && staticpages-cli",
    "test": "jest --config ./jest.config.json",
    "test:update": "jest --config ./jest.config.json -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --quiet --fix"
    ]
  },
  "resolutions": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "js-cookie": "^2.2.1",
    "lazysizes": "^5.2.2",
    "picturefill": "^3.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@4rk/staticpages-cli": "^0.1.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-viewport": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@storybook/theming": "^6.0.26",
    "@types/jest": "^26.0.14",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
