{
  "name": "react-patterns",
  "author": {
    "name": "Fork Unstable Media GmbH",
    "email": "obj@fork.de"
  },
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 8000",
    "build": "build-storybook -o storybook-static && cd storybook-static/ && cp -r ../public/. ./public/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --quiet --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "@4rk/staticpages-cli": "^0.1.4",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/theming": "^5.1.3",
    "lazysizes": "^5.0.0-beta2",
    "picturefill": "^3.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-sprite-loader": "^4.1.6",
    "what-input": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.4.4",
    "@storybook/addon-viewport": "^5.2.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0"
  }
}
